package com.app;

public class Person{
	//Field of Person class
	int age;
	String name;
	String state;
	
	//Constructor of Person class
	public Person(int age, String name, String state) {
		this.age = age;
		this.name = name;
		this.state = state;
	}
	
	public String toString() {
		return "[Age: "+age+" Name: "+name+" State: "+state+"]";
	}

	
}
